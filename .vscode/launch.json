    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Python: simpleRSA_Generate.py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/SimpleRSA/simpleRSA.py",
            "args" : ["-G", "--p", "19", "--q", "23"]
        },
        {
            "name": "Python: simpleRSA_Encrypt.py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/SimpleRSA/simpleRSA.py",
            "args" : ["-E", "--n", "437", "--e", "395", "--plain_text", "love"]
        },
        {
            "name": "Python: simpleRSA_Decrypt.py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/SimpleRSA/simpleRSA.py",
            "args" : ["-D", "--n", "437", "--d", "395",
            "--cypher_text", "174",  "63", "100", "225"]
        },
       
    ]
}